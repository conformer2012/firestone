{
	"name": "firestone",
	"version": "12.5.5",
	"scripts": {
		"ng": "nx",
		"postinstall": "node ./decorate-angular-cli.js && echo \"// https://github.com/microsoft/TypeScript/issues/27956#issuecomment-430849185\" > ./node_modules/@overwolf/ow-cli/index.d.ts && electron-builder install-app-deps",
		"clear-cache": "rm -rf node_modules/.cache/nx",
		"dev": "nx build --source-map=true --watch",
		"dev-web": "nx build website --source-map=true --watch",
		"post-build": "npx ts-node ./build-tools/copy-overwolf-assets.ts && npx ts-node ./build-tools/replace-version.ts",
		"opk-pipeline": "npx ts-node ./build-tools/opk-pipeline.ts",
		"build": "rm -rf node_modules/.cache/nx && nx build --configuration=production --source-map && npm run post-build && npm run opk-pipeline",
		"build-local": "rm -rf node_modules/.cache/nx && nx build --configuration=production --source-map && npm run post-build && npx ts-node ./build-tools/opk-pipeline-no-upload.ts",
		"upload-web": "npx ts-node ./build-tools/upload-website.ts",
		"build-web": "rm -rf node_modules/.cache/nx && nx build website --configuration=production --source-map && npm run upload-web",
		"dev-twitch": "set IS_TWITCH=true&& nx build twitch-extension --configuration=development --watch",
		"build-twitch-watch": "nx build twitch-extension --configuration=production --watch",
		"build-twitch": "rm -rf node_modules/.cache/nx && nx build twitch-extension --configuration=production"
	},
